//  INCLUDES

#include <aiftool.rh>
#include <satui.loc>

//  RESOURCE DEFINITIONS 

/*
-----------------------------------------------------------------------------
    
    AIF_DATA

    Standard EPOC application information structure.

-----------------------------------------------------------------------------
*/
RESOURCE AIF_DATA
    {
    // App UID
    app_uid = 0x101f4ce0;

    // Number of icons
    num_icons = 2;

    // Application capabilities
    hidden = KAppNotHidden;
    embeddability = KAppNotEmbeddable;
    newfile = KAppDoesNotSupportNewFile;
    }

// End of File
